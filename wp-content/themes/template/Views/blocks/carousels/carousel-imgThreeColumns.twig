<section class="carousel-imgThreeColumns">
    {% if title %}
        <div class="container">
            <div class="row">
                <div class="col-12 col-sm-12 col-md-12 col-lg-12 col-lg-12">
                    {% include './blocks/title/h3.twig' %}
                </div>
            </div>
        </div>
    {% endif %}
    <div class="owl-carousel owl-theme owl-imgThreeColumns">
        {% for item in items %}
            <div class="item center">
                <img src="{{ item.img.src }}" alt="{{ item.img.alt }}">
            </div>
        {% endfor %}
    </div>
</section>